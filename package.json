    "build:mobile": "node entry/create-entry.js --mode=mobile && vue-cli-service build --mode mobile",
    "lint": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit",
    "install:tree": "node src/components/tree/install.js && npm i",
    "install:element": "cd ui-libs/element-dev && npm i"
  },
  "husky": {
    "hooks": {}
  },
  "lint-staged": {},
