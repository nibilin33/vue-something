{
  "name": "ume_ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint --fix",
    "build:meeting": "node entry/create-entry.js --mode=reservation && node entry/build-rename/meeting.js && vue-cli-service build --mode build.meeting && node entry/remove-public.js --mode=reservation && node entry/theme-extract.js --mode=reservation",
    "build:meeting-admin": "node entry/create-entry.js --mode=admin && node entry/build-rename/meeting.js --bussiness=admin && vue-cli-service build --mode build.meeting && node entry/remove-public.js --mode=reservation && node entry/theme-extract.js --mode=reservation",
    "build:mobile": "node entry/create-entry.js --mode=mobile && vue-cli-service build --mode mobile && node entry/remove-public.js --mode=mobile&& node entry/theme-extract.js --mode=mobile",
    "build:pc": "node entry/create-entry.js --mode=pc && node entry/build-rename/meeting.js && vue-cli-service build --mode pc && node entry/theme-extract.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "install:element": "cd ui-libs/element-dev && npm i",
    "install:tree": "node src/components/tree/install.js && npm i",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "server": "cross-env NODE_ENV=development supervisor -w server server/app.js",
    "start:mobile": "node entry/create-entry.js --mode=mobile && vue-cli-service serve --mode mobile --open true",
    "start:pc": "node entry/create-entry.js --mode=pc && node entry/build-rename/meeting.js && vue-cli-service serve --mode pc --open true",
    "test:unit": "vue-cli-service test:unit",
    "uglifyjs:element": "uglifyjs ui-lib/element-dev/lib/element-ui.common.js  --compress --mangle  --output ui-lib/element-dev/lib/element-ui.common.js"
  },
  "main": "background.ts",
