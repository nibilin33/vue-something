  "private": true,
  "scripts": {
    "server": "cross-env NODE_ENV=development supervisor -w server server/app.js",
    "start:pc": "node entry/create-entry.js --mode=pc && vue-cli-service serve --mode pc --open true",
    "start:mobile": "node entry/create-entry.js --mode=mobile && vue-cli-service serve --mode mobile --open true",
    "uglifyjs:element": "uglifyjs ui-lib/element-dev/lib/element-ui.common.js  --compress --mangle  --output ui-lib/element-dev/lib/element-ui.common.js",
